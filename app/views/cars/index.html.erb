<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<form method="GET" action="/cars">
Modelo : <input type="text" name="model"/>
<input type="submit"/>
</form>
<br>
<form method="GET" action="/cars">
Color : <input type="text" name="color"/>
<input type="submit"/>
</form>
<br>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Color</th>
      <th>Brand</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.model %></td>
        <td><%= car.color %></td>
        <td><%= car.brand_id %></td>
        <td><%= car.patent %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>


